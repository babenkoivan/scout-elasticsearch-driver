{
    "name": "babenkoivan/scout-elasticsearch-driver",
    "type": "library",
    "description": "The Elasticsearch Driver for Laravel Scout",
    "keywords": [
        "driver",
        "elastic",
        "elasticsearch",
        "engine",
        "laravel",
        "scout",
        "search"
    ],
    "homepage": "https://babenkoivan.github.io/scout-elasticsearch-driver/",
    "support": {
        "issues": "https://github.com/babenkoivan/scout-elasticsearch-driver/issues",
        "source": "https://github.com/babenkoivan/scout-elasticsearch-driver"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Babenko Ivan",
            "email": "babenko.i.a@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "elasticsearch/elasticsearch": "^6.0",
        "laravel/scout": "^7.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^6.0 || ^7.5 || ^8.0"
    },
    "autoload": {
        "psr-4": {
            "ScoutElastic\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ScoutElastic\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0-dev"
        },
        "laravel": {
            "providers": [
                "ScoutElastic\\ScoutElasticServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
